<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<AssemblyVersion>24.2.8.0</AssemblyVersion>
	<Version>24.2.8.0</Version>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<RepositoryUrl>https://github.com/farzadjalalimajd/SfPdfViewer2PropertyEditor</RepositoryUrl>
	<Title>ExpressApp Blazor SfPdfViewer2PropertyEditor</Title>
	<Authors>Farzad Jalali Majd</Authors>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="8.0.17" />
	<PackageReference Include="DevExpress.ExpressApp.Blazor" Version="24.2.8" />
	<PackageReference Include="Syncfusion.Blazor.SfPdfViewer" Version="28.1.33" />
	<PackageReference Include="Syncfusion.Blazor.Themes" Version="28.1.33" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

</Project>
